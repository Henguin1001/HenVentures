
{% obj header_henryisawesome %}
	<header class="dealwithit">
		<div class="dealwithit-container" style="background-image:url('{{image| default('') }}')">
			<h1 align="center" class="dealwithit-heading center">Henry is
			  <span
			     class="txt-rotate"
			     data-period="2000"
			     data-rotate='{{henryis| json_encode }}'></span>
			</h1>
		</div>
	</header>
{% endobj %}

{% obj navbar %}
	{% prototype show %}
		<div class="navbar navbar-default">
			<div class="navbar-header">
				<button type="button" class="navbar-toggle" data-toggle="collapse" data-target=".navbar-responsive-collapse">
					<span class="icon-bar"></span>
					<span class="icon-bar"></span>
					<span class="icon-bar"></span>
				</button>
				<a class="navbar-brand" href="/">Henry Troutman</a>
			</div>
			<div class="navbar-collapse collapse navbar-responsive-collapse">
				<ul class="nav navbar-nav">
					{% if active == 'home' %} {{__methods.home()}}
					{% elseif active == 'about' %} {{__methods.about()}}
					{% elseif active == 'projects' %} {{__methods.projects()}}
					{% endif %}
				</ul>
			</div>
		</div>
	{% endprototype %}
	
	{% prototype home%}
		<li><a href="#about">about</a></li>
		<li><a href="/projects">projects</a></li>
	{% endprototype %}
	{% prototype about%}
		<li class="active"><a href="#about">about</a></li>
		<li><a href="/projects">projects</a></li>
	{% endprototype %}
	{% prototype projects%}
		<li><a href="/#about">about</a></li>
		<li class="active"><a href="/projects">projects</a></li>
	{% endprototype %}
{% endobj %}
